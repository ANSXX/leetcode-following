{
    "manifest_version": 3,

    "name": "LeetCode Following",
    "description": "Follow friends on LeetCode!",
    "version": "0.1",

    "action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
  
    
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    },
    
    "permissions": [
      "storage"
    ]
}



